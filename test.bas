10 REM --- Tiny BASIC Test Program ---
20 PRINT "ENTER A NUMBER:"
30 INPUT N
40 LET X = N * 2
50 PRINT "DOUBLE IS "; X
60 IF X > 10 THEN GOTO 100
70 PRINT "X IS SMALL"
80 GOTO 120

100 PRINT "X IS LARGE"
110 GOSUB 200
120 PRINT "DONE"
130 END

200 REM --- Subroutine Test ---
210 PRINT "IN SUBROUTINE"
220 RETURN
